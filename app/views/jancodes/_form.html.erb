<%= form_with(model: jancode) do |form| %>
  <% if jancode.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(jancode.errors.count, "error") %> prohibited this jancode from being saved:</h2>

      <ul>
        <% jancode.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :JANコード %>
    <%= form.number_field :jancode %>
  </div>

  <div class="field">
    <%= form.label :カテゴリーNo %>
    <%= form.number_field :categoryno %>
  </div>

  <div class="field">
    <%= form.label :商品名 %>
    <%= form.text_area :item %>
  </div>

  <div class="actions">
    <%= form.submit"登録" %>
  </div>
<% end %>
